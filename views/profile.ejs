
<h5>Welcome, <%= userName %> </h5>
<div class="center">
    <h4>My Gallery</h4>
    <div class="row">
        <% faves.forEach( f =>{ %>
            <div class="col l3 s6">
                <img class="cover" src="<%= f.url %>" alt="<%= f.date %>" height="200">
                <p><%= f.date %></p>
                <form method="GET" action="/detail">
                    <input hidden type="date" id="date" name="date" value="<%= f.date %>">
                    <button type="submit">See Detail</button>
                </form>
            </div>
        <%}) %> 
    </div>
</div>



