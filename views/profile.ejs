
<h5>Welcome, <%= userName %> </h5>
<div class="center">
    <h4>My Gallery</h4>
    <div class="row">
        <% faves.forEach( f =>{ %>
            <div class="col l3 s6">
                <img class="cover" src="<%= f.url %>" alt="<%= f.date %>" height="200">
                <span><%= f.date %> </span>
            </div>
        <%}) %> 
    </div>
</div>



