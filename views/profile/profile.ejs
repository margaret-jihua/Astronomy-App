
<h5>Welcome, <%= userName %> </h5>
<div class="center">
    <h4>My Gallery</h4>
    <div class="row">
        <% faves.forEach( f =>{ %>
            <div class="col l3 m6 s12">
                <% if (f.url.substring(8,11) === 'www') { %> 
                    <iframe class="cover" src="<%= f.url %>" height="200"></iframe>
                <% } else { %> 
                    <img class="cover" src="<%= f.url %>" alt="<%= f.date %>" height="200">
                <% } %> 

                <p><%= f.date %></p>

                <div class="flex">
                    <a class="profile-btn" href="/profile/<%= f.id %>">
                        <button>View</button>
                    </a>
                    <form class="profile-btn" method="POST" action="/profile/<%= f.id %>/?_method=DELETE"">
                        <input hidden type="text" name="id" value="<%= f.id %>">
                        <button type="submit">Delete</button>
                    </form>
                </div>
            </div>
        <% }) %> 
    </div>
</div>



